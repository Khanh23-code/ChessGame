<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>disable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\BackgroundMenu.jpeg" />
    <None Remove="Assets\BishopB.png" />
    <None Remove="Assets\BishopW.png" />
    <None Remove="Assets\Board.png" />
    <None Remove="Assets\CursorB.cur" />
    <None Remove="Assets\CursorW.cur" />
    <None Remove="Assets\GoogleIcon.png" />
    <None Remove="Assets\icon.ico" />
    <None Remove="Assets\KingB.png" />
    <None Remove="Assets\KingW.png" />
    <None Remove="Assets\KnightB.png" />
    <None Remove="Assets\KnightW.png" />
    <None Remove="Assets\PawnB.png" />
    <None Remove="Assets\PawnW.png" />
    <None Remove="Assets\QueenB.png" />
    <None Remove="Assets\QueenW.png" />
    <None Remove="Assets\RookB.png" />
    <None Remove="Assets\RookW.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="6.1.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ChessLogic\ChessLogic.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\LoginAssets\BackgroundMenu.jpeg" />
    <Resource Include="Assets\BoardAsssets\BishopB.png" />
    <Resource Include="Assets\BoardAsssets\BishopW.png" />
    <Resource Include="Assets\BoardAsssets\Board.png" />
    <Resource Include="Assets\BoardAsssets\CursorB.cur" />
    <Resource Include="Assets\BoardAsssets\CursorW.cur" />
    <Resource Include="Assets\LoginAssets\GoogleIcon.png" />
    <Resource Include="Assets\BoardAsssets\icon.ico" />
    <Resource Include="Assets\BoardAsssets\KingB.png" />
    <Resource Include="Assets\BoardAsssets\KingW.png" />
    <Resource Include="Assets\BoardAsssets\KnightB.png" />
    <Resource Include="Assets\BoardAsssets\KnightW.png" />
    <Resource Include="Assets\BoardAsssets\PawnB.png" />
    <Resource Include="Assets\BoardAsssets\PawnW.png" />
    <Resource Include="Assets\BoardAsssets\QueenB.png" />
    <Resource Include="Assets\BoardAsssets\QueenW.png" />
    <Resource Include="Assets\BoardAsssets\RookB.png" />
    <Resource Include="Assets\BoardAsssets\RookW.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
