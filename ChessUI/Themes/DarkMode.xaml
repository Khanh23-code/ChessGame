    <SolidColorBrush x:Key="Brush.Background.Secondary" Color="#252525" />
    <SolidColorBrush x:Key="Brush.Background.Tertiary" Color="#353535" />
    <SolidColorBrush x:Key="Brush.Glass" Color="#CC1c1c1c" />
    <SolidColorBrush x:Key="Brush.Text.Primary" Color="#ffffffff" />
    <SolidColorBrush x:Key="Brush.Text.Secondary" Color="#AAAAAA" />
    <SolidColorBrush x:Key="Brush.Text.Accent" Color="#00DC82" />
    <SolidColorBrush x:Key="Brush.Border.Light" Color="#ffeeeeee" />
